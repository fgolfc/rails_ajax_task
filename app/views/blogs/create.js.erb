$("#favorite-button_<%= @blog.id %>").html("<%= j(render 'blog/create', blog: @blog) %>");
$("#favorite-count_<%= @blog.id %>").html('<%= link_to "#{ @user.favorites.length }", favorite_path(@blog) %>');
$("#favorite-count_<%= current_user.id %>").html('<%= link_to "#{ current_user.favorites.length }", blog_favorites_path(@blog) %>');